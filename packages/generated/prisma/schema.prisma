generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum VideoStatus {
  ready
  errored
  processing
}

model Video {
  id         String       @id @default(cuid())
  videoUrl   String?
  status     VideoStatus?
  progress   Float        @default(0)
  inputProps Json
  createdAt  DateTime     @default(now())
}
